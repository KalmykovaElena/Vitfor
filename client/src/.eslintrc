{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-param-reassign": 0,
    "no-console":0,
    "import/prefer-default-export": "off",
    "prettier/prettier": ["error",{ "endOfLine": "auto" }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
